{
  "extends": ["airbnb-base"],
  "rules": {
    "max-len": 0,
    "no-console": 0,
    "no-unused-vars": 0,
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120
      }
    ]
  },
  "plugins": ["prettier"]
}