{
  "name": "js-training",
  "version": "0.9.1",
  "description": "JS training tasks",
  "scripts": {
    "lint": "eslint task/*.js",
    "precommit": "lint-staged",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --require babel-polyfill",
    "test-debug": "./node_modules/.bin/mocha --debug-brk --compilers js:babel-core/register --require babel-polyfill"
  },
  "author": "aorgish",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "lint-staged": "^6.1.0",
    "mocha": "^2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AliakseiBychyk/js-assignments.git"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  }
}
